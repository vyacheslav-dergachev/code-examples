{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^3.5",
        "league/object-mapper": "dev-main",
        "nelmio/api-doc-bundle": "*",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/asset": "~7.3.0",
        "symfony/asset-mapper": "~7.3.0",
        "symfony/console": "~7.3.0",
        "symfony/doctrine-messenger": "~7.3.0",
        "symfony/dotenv": "~7.3.0",
        "symfony/expression-language": "~7.3.0",
        "symfony/flex": "^2",
        "symfony/form": "~7.3.0",
        "symfony/framework-bundle": "~7.3.0",
        "symfony/http-client": "~7.3.0",
        "symfony/intl": "~7.3.0",
        "symfony/mailer": "~7.3.0",
        "symfony/mime": "~7.3.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "~7.3.0",
        "symfony/process": "~7.3.0",
        "symfony/property-access": "~7.3.0",
        "symfony/property-info": "~7.3.0",
        "symfony/runtime": "~7.3.0",
        "symfony/security-bundle": "~7.3.0",
        "symfony/serializer": "~7.3.0",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "~7.3.0",
        "symfony/translation": "~7.3.0",
        "symfony/twig-bundle": "~7.3.0",
        "symfony/uid": "~7.3.0",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "~7.3.0",
        "symfony/web-link": "~7.3.0",
        "symfony/yaml": "~7.3.0",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.14",
        "ergebnis/composer-normalize": "^2.48",
        "phpro/grumphp": "*",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.4",
        "qossmic/deptrac-shim": "^1.0",
        "slevomat/coding-standard": "^8.24",
        "symfony/browser-kit": "~7.3.0",
        "symfony/css-selector": "~7.3.0",
        "symfony/debug-bundle": "~7.3.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "~7.3.0",
        "symfony/web-profiler-bundle": "~7.3.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/",
            "UserInterface\\": "src/UserInterface/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
